									<occurrences minimum="1" maximum="1"/>
									<help-text/>
								</input>
								<input name="button" type="CheckBox">
									<label i18n="promoted-banner.button">Vis som knapp</label>
									<occurrences minimum="0" maximum="1"/>
									<help-text/>
								</input>
								<option-set name="linkType">
									<label i18n="link.type">Type of link</label>
									<expanded>true</expanded>
									<occurrences minimum="1" maximum="1"/>
									<options minimum="1" maximum="1">
										<option name="internal">
											<label i18n="link.internal">Internal</label>
											<default>true</default>
											<items>
												<input type="ContentSelector" name="id">
													<label i18n="content-choose">Choose content</label>
													<occurrences minimum="1" maximum="1"/>
													<config>
														<allowContentType>activity</allowContentType>
														<allowContentType>activity-event</allowContentType>
														<allowContentType>article</allowContentType>
														<allowContentType>education</allowContentType>
														<allowContentType>feature</allowContentType>
														<allowContentType>initial-service</allowContentType>
														<allowContentType>initial-service-category</allowContentType>
														<allowContentType>initial-service-location</allowContentType>
														<allowContentType>job-ad</allowContentType>
														<allowContentType>landing-page</allowContentType>
														<allowContentType>news</allowContentType>
														<allowContentType>press-release</allowContentType>
														<allowContentType>project</allowContentType>
														<allowContentType>research-article</allowContentType>
														<allowContentType>research-institution</allowContentType>
														<allowContentType>research-profile</allowContentType>
														<allowContentType>media:document</allowContentType>
														<allowContentType>media:spreadsheet</allowContentType>
														<allowContentType>media:presentation</allowContentType>
														<allowPath>${site}/*</allowPath>
													</config>
													<help-text/>
												</input>
												<input name="anchor" type="TextLine">
													<label i18n="anchor">Anchor</label>
													<occurrences minimum="0" maximum="1"/>
