								<max-length>120</max-length>
							</config>
							<occurrences minimum="0" maximum="1"/>
							<help-text/>
						</input>
					</items>
				</option>
				<option name="quote">
					<label i18n="quote">Sitat</label>
					<items>
						<input name="text" type="TextArea">
							<label i18n="text">Tekst</label>
							<occurrences minimum="1" maximum="1"/>
							<help-text/>
						</input>
						<input name="footer" type="TextArea">
							<label i18n="quote.cite">Sitering</label>
							<occurrences minimum="0" maximum="1"/>
							<help-text/>
						</input>
					</items>
				</option>
				<option name="fact-big">
					<label i18n="fact">Fakta</label>
					<items>
						<input name="title" type="TextLine">
							<label i18n="title">Title</label>
							<indexed>true</indexed>
							<occurrences minimum="0" maximum="1"/>
