													<config>
														<alignment>right</alignment>
													</config>
													<occurrences minimum="0" maximum="1"/>
													<help-text/>
												</input>
											</items>
										</option>
									</options>
								</option-set>
							</items>
						</item-set>
					</items>
				</option>
				<option name="video-banner">
