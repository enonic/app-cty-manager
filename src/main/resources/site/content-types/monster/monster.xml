<?xml version="1.0"?>
<content-type>
	<display-name-label/>
	<display-name-expression/>
	<description/>
	<is-abstract>false</is-abstract>
	<is-final>false</is-final>
	<is-built-in>false</is-built-in>
	<allow-child-content>true</allow-child-content>
	<display-name i18n="article.displayName">Artikkel</display-name>
	<super-type>base:structured</super-type>
	<form>
		<item-set name="cover">
			<label i18n="article-base.cover">Cover Edited</label>
			<label i18n="cover">Cover</label>
			<occurrences minimum="0" maximum="1"/>
			<items>
				<option-set name="type">
					<label i18n="media">Media</label>
					<occurrences minimum="1" maximum="1"/>
					<options minimum="1" maximum="1">
						<option name="image">
							<label i18n="image">Bilde</label>
							<items>
								<input name="id" type="ImageSelector">
									<label i18n="content-choose">Velg innhold</label>
									<occurrences minimum="1" maximum="1"/>
									<config>
										<allowPath>${site}/*</allowPath>
									</config>
									<help-text/>
								</input>
							</items>
						</option>
						<option name="video">
							<label i18n="video">Video</label>
							<items>
								<input name="id" type="ContentSelector">
									<label i18n="content-choose">Velg innhold</label>
									<occurrences minimum="1" maximum="1"/>
									<config>
										<allowContentType>video</allowContentType>
										<allowContentType>video-qbrick</allowContentType>
										<allowPath>${site}/*</allowPath>
									</config>
									<help-text/>
								</input>
							</items>
						</option>
					</options>
				</option-set>
				<input name="aside" type="CheckBox">
					<label i18n="aside">Right align cover</label>
					<occurrences minimum="0" maximum="1"/>
					<help-text/>
				</input>
			</items>
		</item-set>
		<item-set name="background">
			<label i18n="background">Bakgrunn</label>
			<occurrences minimum="0" maximum="1"/>
			<items>
				<input type="CustomSelector" name="color">
					<label i18n="background.color">Bakgrunn</label>
					<occurrences minimum="0" maximum="1"/>
					<config>
						<service>background-color-service</service>
					</config>
					<help-text/>
				</input>
				<input name="twoTone" type="CheckBox">
					<label i18n="background.twotone">2-tone bakgrunn</label>
					<occurrences minimum="0" maximum="1"/>
					<help-text/>
				</input>
			</items>
		</item-set>
		<input name="preface" type="TextArea">
			<label i18n="article-base.preface">Ingress</label>
			<config>
				<max-length>300</max-length>
			</config>
			<occurrences minimum="0" maximum="1"/>
			<help-text/>
		</input>
		<input name="byline" type="HtmlArea">
			<label i18n="article-base.byline">Byline</label>
			<occurrences minimum="0" maximum="1"/>
			<config>
				<exclude>*</exclude>
				<include>Bold</include>
			</config>
			<help-text/>
		</input>
		<input name="showPublish" type="CheckBox">
			<label i18n="article-base.show-publish">Vis publisert dato</label>
			<occurrences minimum="0" maximum="1"/>
			<help-text/>
		</input>
		<input name="body" type="HtmlArea">
			<label i18n="article-base.body">Brødtekst</label>
			<occurrences minimum="0" maximum="1"/>
			<config>
				<include>Blockquote</include>
				<exclude>Code | Table</exclude>
				<allowHeadings>h2</allowHeadings>
			</config>
			<help-text/>
		</input>
		<option-set name="blocks">
			<label i18n="block">Blokk</label>
			<expanded>false</expanded>
			<occurrences minimum="0" maximum="0"/>
			<options minimum="1" maximum="1">
				<option name="body">
					<label i18n="article-base.body">Brødtekst</label>
					<items>
						<input name="text" type="HtmlArea">
							<label i18n="text">Tekst</label>
							<occurrences minimum="1" maximum="1"/>
							<config>
								<exclude>Code | Blockquote | Table</exclude>
								<allowHeadings>h2</allowHeadings>
							</config>
							<help-text/>
						</input>
					</items>
				</option>
				<option name="carousel">
					<label i18n="carousel">Karusell</label>
					<items>
						<item-set name="images">
							<label i18n="image">Media</label>
							<occurrences minimum="3" maximum="10"/>
							<items>
								<input name="id" type="ImageSelector">
									<label i18n="content-choose">Velg innhold</label>
									<occurrences minimum="1" maximum="1"/>
									<config>
										<allowPath>${site}/*</allowPath>
									</config>
									<help-text/>
								</input>
								<input name="text" type="TextArea">
									<label i18n="text">Tekst</label>
									<occurrences minimum="0" maximum="1"/>
									<help-text/>
								</input>
							</items>
						</item-set>
					</items>
				</option>
				<option name="promoted-banner">
					<label i18n="promoted-banner">Promotert banner</label>
					<items>
						<item-set name="cover">
							<label i18n="cover">Cover</label>
							<occurrences minimum="0" maximum="1"/>
							<items>
								<input name="image" type="ImageSelector">
									<label i18n="content-choose">Velg innhold</label>
									<occurrences minimum="1" maximum="1"/>
									<config>
										<allowPath>${site}/*</allowPath>
									</config>
									<help-text/>
								</input>
								<input name="fullwidth" type="CheckBox">
									<label i18n="promoted-banner.fullwidth">Vis i fullbredde</label>
									<default>checked</default>
									<occurrences minimum="0" maximum="1"/>
									<help-text/>
								</input>
								<input name="overlay" type="CheckBox">
									<label i18n="promoted-banner.overlay">Gjør bildet mørkere</label>
									<occurrences minimum="0" maximum="1"/>
									<help-text/>
								</input>
							</items>
						</item-set>
						<input type="CustomSelector" name="color">
							<label i18n="background">Bakgrunn</label>
							<occurrences minimum="0" maximum="1"/>
							<config>
								<service>background-color-service</service>
								<param value="exclude">beige-light</param>
							</config>
							<help-text/>
						</input>
						<input name="text" type="TextArea">
							<label i18n="text">Text</label>
							<indexed>true</indexed>
							<config>
								<max-length>80</max-length>
							</config>
							<occurrences minimum="0" maximum="1"/>
							<help-text/>
						</input>
						<input type="TextLine" name="cite">
							<label i18n="promoted-banner.cite">Sitering</label>
							<occurrences minimum="0" maximum="1"/>
							<help-text/>
						</input>
						<input name="label" type="TextLine">
							<label i18n="label">Label</label>
							<occurrences minimum="0" maximum="1"/>
							<help-text/>
						</input>
						<item-set name="link">
